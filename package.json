{
  "name": "conflux-bounty",
  "version": "1.0.0",
  "description": "Conflux Bounty",
  "scripts": {
    "start": "cross-env NODE_ENV=development BOUNTY_BACKEND_ENDPOINT=http://bountytest.conflux-chain.org webpack-dev-server --config webpack.dev.config.js",
    "dev": "npm run start",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "lint": "concurrently --names 'JS,STYLE' -c 'green,blue' 'npm run lint:eslint .' 'npm run lint:style'",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "eslint . --ignore-path .gitignore --fix",
    "lint:style": "stylelint src/**/*.js",
    "lint:style:fix": "stylelint src/**/* --fix",
    "lintfix": "prettier-eslint \"./src/**/*.js\" --write --print-width=100",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [],
  "author": "Angelia",
  "license": "ISC",
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint . --fix",
      "git add"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-recommended"
  },
  "prettier": {
    "printWidth": 140,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always"
  },
  "resolutions": {
    "lodash.merge": "^4.6.2",
    "set-value": "^2.0.1"
  },
  "dependencies": {
    "@sentry/browser": "^5.5.0",
    "ali-oss": "^6.1.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "echarts": "^4.2.1",
    "history": "^4.9.0",
    "html-loader": "^0.5.5",
    "icon-font-loader": "^2.0.3",
    "jest-css-modules": "^2.1.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17",
    "markdown-loader": "^5.0.0",
    "materialize-css": "^1.0.0",
    "moment": "^2.24.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "px2rem-loader": "^0.1.9",
    "qrcode": "^1.3.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-google-recaptcha": "^2.0.1",
    "react-intl": "^3.3.0",
    "react-mobile-image-viewer": "^1.2.2",
    "react-onclickoutside": "^6.8.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-slick": "^0.24.0",
    "react-use": "^12.5.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rmc-picker": "^5.0.10",
    "sanitize-html": "^1.20.1",
    "slick-carousel": "^1.8.1",
    "spark-md5": "^3.0.0",
    "styled-components": "^4.2.0",
    "superagent": "^5.0.6",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "uuid": "^3.3.2",
    "webpack-cdn-plugin": "^3.1.3",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.11.0",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.2",
    "coveralls": "^3.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "is-json": "^2.0.1",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": ">=8",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier-eslint-cli": "^4.7.1",
    "raf": "^3.4.1",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.3",
    "sass": "^1.21.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
